{
  "version": 2,
  "builds": [
    {
      "src": "board/(.*)",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.8",
        "installCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "board/run.py"
    }
  ]
}